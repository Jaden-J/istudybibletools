<?xml version="1.0" encoding="utf-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" loadImage="GetImage">
	<ribbon>
		<tabs>
			<tab id="tabCustom" label="BibleNote">
				<group id="Tools" label="Инструменты" image="NoteLinkerButton.png">
					<button id="VersePointerButton" label="Открыть стих" size="large" screentip="Нажмите, чтобы перейти к нужному месту Писания" onAction="ButtonClick" image="VersePointerButton.png"/>
          <button id="VerseLinkerButton" label="Написать комментарий" size="large" screentip="Нажмите, чтобы оставить комментарий" onAction="ButtonClick" image="VerseLinkerButton.png"/>
          <button id="NoteLinkerButton" label="Анализ заметок" size="large" screentip="Нажмите, чтобы запустить анализ заметок" onAction="ButtonClick" image="NoteLinkerButton.png"/>
          <button id="QuickNoteLinkerButton" label="Быстрый анализ текущей страницы" size="large" screentip="Нажмите, чтобы запустить быстрый анализ текущей страницы" onAction="ButtonClick" image="QuickAnalyze.png"/>
        </group>
        <group id="Search" label="Поиск">
          <button id="SearchInDictionaries" label="Поиск в словарях" size="large" screentip="Нажмите, чтобы начать поиск в словарях" onAction="ButtonClick" image="Dictionary.png"/>
        </group>
        <group id="Unlock" label="Разблокирование Библии" image="UnlockFile.png">
          <button id="UnlockCurrentSection" label="Разблокировать текущую книгу" size="large" screentip="Нажмите, чтобы разблокировать текущую книгу Библии" onAction="ButtonClick" image="UnlockFile.png"/>
          <button id="UnlockAllBible" label="Разблокировать всю Библию" size="large" screentip="Нажмите, чтобы разблокировать всю Библию" onAction="ButtonClick" image="UnlockFolder.png"/>
        </group>
        <group id="General" label="Параметры" image="ConfigureButton.png">
          <button id="ConfigureButton" label="Параметры" size="large" screentip="Нажмите, чтобы изменить параметры" onAction="ButtonClick" image="ConfigureButton.png"/>
          <button id="HelpButton" label="Справка" size="normal" screentip="Нажмите, чтобы открыть файл справки" onAction="ButtonClick" image="HelpButton.png"/>
          <button id="ModuleInfoButton" label="О модуле" size="normal" screentip="Нажмите, чтобы просмотреть информацию о текущем модуле" onAction="ButtonClick" image="AboutModule.png"/>
          <button id="AboutProgramButton" label="О программе" size="normal" screentip="Нажмите, чтобы просмотреть информацию о программе" onAction="ButtonClick" image="AboutProgram.png"/>
        </group>
			</tab>
		</tabs>
	</ribbon>
</customUI>